{
  "name": "Xtension",
  "description": "Extension with cool features",
  "version": "0.1.0",
  "chrome_url_overrides": {
    "newtab":  "./html/newtab.html"
  },
  "manifest_version": 2,
  "background": {
    "scripts": [ "./scripts/background.js" ]
  },
  "options_page": "./html/options.html",
  "browser_action": {
    "default_popup": "./html/popup.html"
  },
  "permissions": [
    "tabs",
    "<all_urls>"
  ]
}